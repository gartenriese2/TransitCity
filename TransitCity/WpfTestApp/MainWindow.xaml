<Window x:Class="WpfTestApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:panel="clr-namespace:WpfDrawing.Panel;assembly=WpfDrawing"
        xmlns:wpfTestApp="clr-namespace:WpfTestApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="800">
    <Window.DataContext>
        <wpfTestApp:MainWindowViewModel/>
    </Window.DataContext>
    <!--<panel:PanelControl Elements="{Binding PanelElements}"/>-->
    <wpfTestApp:DataDisplay6Control HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    DataPoints="{Binding DataPoints}"/>
</Window>
